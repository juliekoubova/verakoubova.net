{
  "name": "@verakoubova/web",
  "version": "1.0.0",
  "author": "Julie Koubov√° <julie@koubova.net>",
  "license": "PRIVATE",
  "private": true,
  "scripts": {
    "clean": "shx rm -rf _site .cache",
    "build": "cross-env NODE_ENV=production run-s clean build:parcel build:eleventy build:csso",
    "build:eleventy": "eleventy --output _site",
    "build:parcel": "parcel build _assets/main.mjs --dist-dir _site",
    "build:csso": "csso --input ./_site/main.css --output ./_site/main.css --comments none --force-media-merge --stat",
    "dev": "npm-run-all -s clean -p start dev:*",
    "dev:parcel": "parcel watch _assets/main.mjs --dist-dir _site --no-hmr",
    "debug": "node --inspect-brk ./node_modules/.bin/eleventy",
    "predeploy": "shx rm -f _site/report.html",
    "serve": "run-s build && ws --directory _site --port 8080",
    "start": "eleventy --serve --quiet --output _site"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@mightyplow/eleventy-plugin-cache-buster": "^1.1.3",
    "@types/autoprefixer": "^9.7.2",
    "@types/js-yaml": "^3.12.5",
    "@verakoubova/navigation": "0.0.0",
    "@verakoubova/plugins": "0.0.0",
    "@verakoubova/polyfills": "0.0.0",
    "@verakoubova/responsiver": "0.0.0",
    "@verakoubova/vertical-rhythm": "0.0.0",
    "autoprefixer": "^9.8.6",
    "csso": "^4.2.0",
    "csso-cli": "^3.0.0",
    "eleventy-plugin-safe-external-links": "^0.1.5",
    "fast-glob": "^3.2.4",
    "jest": "^26.6.3",
    "js-yaml": "^3.14.1",
    "local-web-server": "^4.2.1",
    "parcel": "^2.0.0-beta.1",
    "postcss": "^7.0.35",
    "probe-image-size": "^6.0.0",
    "tailwind-vertical-rhythm": "^1.2.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "tailwindcss-scroll-snap": "^1.0.3",
    "ts-jest": "^26.4.4"
  },
  "dependencies": {
    "@stimulus/mutation-observers": "^2.0.0",
    "@types/turbolinks": "^5.2.0",
    "eleventy-plugin-images-responsiver": "^1.8.2",
    "logrocket": "^1.0.14",
    "slugify": "^1.4.6",
    "stimulus": "^2.0.0",
    "turbolinks": "^5.2.0"
  },
  "browser": "_site/main.js"
}