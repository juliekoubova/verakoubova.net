{
  "name": "@verakoubova/web",
  "version": "1.0.0",
  "author": "Julie Koubov√° <julie@koubova.net>",
  "license": "PRIVATE",
  "private": true,
  "scripts": {
    "clean": "shx rm -rf _site .cache",
    "build": "cross-env NODE_ENV=production run-s clean build:webpack build:eleventy",
    "build:eleventy": "eleventy --output _site",
    "build:webpack": "webpack --mode production",
    "debug": "node --inspect-brk ./node_modules/.bin/eleventy",
    "predeploy": "shx rm -f _site/report.html",
    "serve": "run-s build && ws --directory _site --port 8080",
    "start": "npm-run-all -s clean -p start:*",
    "start:eleventy": "eleventy --serve --quiet --output _site",
    "start:webpack": "webpack --mode development --watch"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@mightyplow/eleventy-plugin-cache-buster": "^1.1.3",
    "@types/autoprefixer": "^9.7.2",
    "@types/js-yaml": "^3.12.5",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@verakoubova/navigation": "0.0.0",
    "@verakoubova/plugins": "0.0.0",
    "@verakoubova/polyfills": "0.0.0",
    "@verakoubova/responsiver": "0.0.0",
    "@verakoubova/vertical-rhythm": "0.0.0",
    "autoprefixer": "^10.2.1",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "eleventy-plugin-safe-external-links": "^0.1.5",
    "fast-glob": "^3.2.4",
    "jest": "^26.6.3",
    "js-yaml": "^3.14.1",
    "local-web-server": "^4.2.1",
    "mini-css-extract-plugin": "^1.3.3",
    "postcss": "^8.2.4",
    "postcss-csso": "^5.0.0",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "probe-image-size": "^6.0.0",
    "tailwind-vertical-rhythm": "^1.2.0",
    "tailwindcss": "^2.0.2",
    "tailwindcss-scroll-snap": "^1.0.3",
    "ts-jest": "^26.4.4",
    "webpack": "^5.12.2",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1"
  },
  "dependencies": {
    "@stimulus/mutation-observers": "^2.0.0",
    "@types/turbolinks": "^5.2.0",
    "eleventy-plugin-images-responsiver": "^1.8.2",
    "logrocket": "^1.0.14",
    "slugify": "^1.4.6",
    "stimulus": "^2.0.0",
    "turbolinks": "^5.2.0"
  }
}
