<link rel="canonical" href="{{ site.publicUrl }}{{ page.url | url }}">
{% for item in site.languages | dictsort %}
{% set targetPageUrl = pageId | urlOfPageId(item[0]) %}
{% if targetPageUrl %}
<link rel="alternate" hreflang="{{ item[0] }}" href="{{ site.publicUrl }}{{ targetPageUrl }}">
{% endif %}
{% endfor %}