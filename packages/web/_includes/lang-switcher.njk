<nav class="flex-none ml-4">
  <ul class="vr-normal-sm">
  {% for item in page | languages | sort %}
    <li class="inline pipe-separator">
      {% if item.active %}
      <strong>{{ item.label }}</strong>
      {% else %}
      <a href="{{ item.url | url }}">{{ item.label }}</a>
      {% endif %}
    </li>
  {% endfor %}
  </ul>
</nav>